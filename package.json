{
  "name": "nerevu-analytics",
  "version": "0.26.0",
  "license": "MIT",
  "description": "Nerevu Analytics website",
  "repository": {
    "type": "git",
    "url": "https://github.com/nerevu/analytics.nerevu.com"
  },
  "engines": {
    "node": ">=8.7.0,<0.0.0"
  },
  "config": {
    "site": "local",
    "serve": true
  },
  "scripts": {
    "clean": "rimraf README.md source plugins layouts data package.json *.coffee",
    "start": "SITE=$npm_package_config_site SERVE=$npm_package_config_serve coffee build.coffee",
    "start:netlify": "NODE_ENV=production npm run start --nerevu:site=netlify --nerevu:serve=false",
    "lint": "coffeelint **/*.coffee *.coffee",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "async": "^1.5.2",
    "axis": "^0.6.1",
    "browserify": "^13.1.0",
    "coffeeify": "^2.0.1",
    "coffeescript": "^1.11.0",
    "jeet": "^6.1.2",
    "marked": "^0.4.0",
    "metalsmith": "^2.3.0",
    "metalsmith-beautify": "^0.2.0",
    "metalsmith-mithril": "^0.2.3",
    "mithril": "^1.1.6",
    "mithril-node-render": "^2.3.1",
    "moment-timezone": "^0.5.23",
    "node-static": "^0.7.9",
    "rimraf": "^2.5.4",
    "stylus": "^0.54.5",
    "urijs": "^1.19.1"
  },
  "devDependencies": {
    "http-status": "^0.2.3",
    "coffeelint": "^1.16.0",
    "metalsmith-clean": "^0.0.0",
    "metalsmith-fingerprint-ignore": "^1.1.2",
    "metalsmith-html-minifier": "^2.1.0",
    "metalsmith-if": "^0.1.1",
    "metalsmith-linkcheck": "^0.2.8",
    "metalsmith-livereload": "^0.1.0",
    "metalsmith-permalinks": "^2.0.0",
    "metalsmith-uglify": "^2.2.0",
    "multimatch": "^2.1.0",
    "winston": "^2.3.0"
  }
}
